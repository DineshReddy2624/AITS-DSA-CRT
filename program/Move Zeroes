class Solution {
    public void moveZeroes(int[] nums) {
        int a = 0;
        int n = nums.length;
        for(int i = 0 ; i<=n-1;i++)
        {
            if(nums[i] != 0)
            {
                nums[a] = nums[i];
                a++;
            }
        }
        for(int i =a ;i<nums.length;i++)
        {
            nums[i] = 0;
        }
    }
}
